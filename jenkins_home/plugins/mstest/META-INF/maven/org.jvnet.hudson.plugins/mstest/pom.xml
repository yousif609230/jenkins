<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.jvnet.hudson.plugins</groupId>
  <artifactId>mstest</artifactId>
  <version>1.0.5</version>
  <packaging>hpi</packaging>
  <name>MSTest plugin</name>
  <description>Generates test reports for MSTest.</description>
  <url>http://wiki.jenkins-ci.org/display/JENKINS/MSTest+Plugin</url>
  <inceptionYear>2016</inceptionYear>
  <developers>
    <developer>
      <id>nilleb</id>
      <name>Ivo Bellin Salarin</name>
      <email>me@nilleb.com</email>
    </developer>
    <developer>
      <id>casz</id>
      <name>Joseph Petersen</name>
      <url>https://github.com/casz/ama</url>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:https://github.com:jenkinsci/mstest-plugin.git</connection>
    <developerConnection>scm:git:git@github.com:jenkinsci/mstest-plugin.git</developerConnection>
    <tag>mstest-1.0.5</tag>
    <url>https://github.com/jenkinsci/mstest-plugin</url>
  </scm>
  <distributionManagement>
    <repository>
      <id>maven.jenkins-ci.org</id>
      <url>https://repo.jenkins-ci.org/releases/</url>
    </repository>
    <snapshotRepository>
      <id>maven.jenkins-ci.org</id>
      <url>https://repo.jenkins-ci.org/snapshots/</url>
    </snapshotRepository>
  </distributionManagement>
  <dependencies>
    <dependency>
      <groupId>org.jenkins-ci.plugins</groupId>
      <artifactId>junit</artifactId>
      <version>1240.vf9529b_881428</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.jenkins-ci.tools</groupId>
        <artifactId>maven-hpi-plugin</artifactId>
        <version>3.49</version>
        <extensions>true</extensions>
      </plugin>
    </plugins>
